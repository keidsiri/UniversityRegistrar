<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>K&J University </title>
  <title>Registrar System</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css"
    integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
  <link rel="stylesheet" href="~/css/styles.css">
  <link rel="stylesheet" href="~/css/styles.css">
</head>
<body>
  @await Html.PartialAsync("NavBar")  
  <div class="bg-transparent text-secondary px-4 py-5 text-center" id="home-index">
    <div class="py-5">
      <h1 class="display-5 fw-bold text-white">K&J University</h1>
      <div class="col-lg-6 mx-auto">
        <p class="fs-5 mb-4">Your resource for course and student lists!</p>
        <div class="d-grid gap-2 d-sm-flex justify-content-sm-center">
        </div>
      </div>
    </div>
  </div>
  <div class="row text-center" id="home-buttons">
    <div class="col-sm-6">
      <div class="main-link-img" id="students-img"></div>
      <h2>See all students</h2>
      <p><a class="btn btn-secondary" href="Students">View details »</a></p>
    </div>
    <div class="col-sm-6">
      <div class="main-link-img" id="courses-img"></div>
      <h2>See all courses</h2>
      <p><a class="btn btn-secondary" href="Courses">View details »</a></p>
    </div>
  </div>
  @await Html.PartialAsync("Footer")
</body>
</html>
